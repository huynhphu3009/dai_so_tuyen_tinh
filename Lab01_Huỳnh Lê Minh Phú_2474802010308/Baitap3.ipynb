   "source": [
    "from sympy import Symbol, solve\n",
    "x= Symbol('x')\n",
    "a= Symbol('a')\n",
    "b= Symbol('b')\n",
    "c= Symbol('c')\n",
    "ptb2= a*x*x + b*x + c\n",
    "nghiem= solve(ptb2, x, dict=True)\n",
    "print(nghiem)\n",
    "#Kết quả: [{x: (-b - sqrt(-4*a*c + b**2))/(2*a)}, {x: (-b + sqrt(-4*a*c + b**2))/(2*a)}]"
   ]
